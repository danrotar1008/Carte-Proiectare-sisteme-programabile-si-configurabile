# JA

NET "port_intrare1<0>" LOC = P81;	# 1 I/O pin
NET "port_intrare1<1>" LOC = P91;	# 2 I/O pin
NET "port_intrare1<2>" LOC = P82;	# 3 I/O pin
NET "port_intrare1<3>" LOC = P92;	# 4 I/O pin
# ATENTZIE !!! Fatza de varianta NEXIS3, lipseste port_intrare1<4>, port_intrare1<5>, port_intrare1<6>, port_intrare1<7>

